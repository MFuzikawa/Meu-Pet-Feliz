
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
  model Servico {
    id              String @id @default(uuid())
    nome            String
    preco           Float
    descricao       String?
    destaque        Boolean
    duracaoEmMinutos  Int

}
  model agendamento {
    id              String @id @default(uuid())
    clienteId       String
    servico         Servico
  }

