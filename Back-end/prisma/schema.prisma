
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
  model Servico {
    id              String @id @default(uuid())
    nome            String
    preco           Float
    descricao       String?
    destaque        Boolean
    horasDeServico  Int
}
